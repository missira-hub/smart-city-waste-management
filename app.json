{
  "expo": {
    "name": "smart-city-waste-management",
    "slug": "smart-city-waste-management",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png", 
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-icon.png",  
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/image/icon.png",  
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "INTERNET"
      ],
      "package": "com.example.smartcitywastemanagement",
      "versionCode": 1,
      "config": {
        "googleMaps": {
          "apiKey": "YOUR_ANDROID_API_KEY"
    },
    "jsEngine": "jsc",  
    "assetBundlePatterns": ["**/*"]  
  },
  "web": {
    "favicon": "./src/assets/favicon.png"  
    }
  }
}
}
    

